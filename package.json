{
    "name": "backlogger",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
        "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
        "analyze": "cross-env ANALYZE=true next build",
        "build": "next build",
        "dev": "next dev",
        "lint:pages": "eslint --fix './pages/**/*.{ts,tsx}'",
        "lint:src": "eslint --fix './src/**/*.{ts,tsx}'",
        "lint": "yarn run lint:src && yarn run lint:pages",
        "test": "SKIP_PREFLIGHT_CHECK=true react-scripts test --env=jest-environment-jsdom-sixteen"
    },
    "dependencies": {
        "@apollo/react-hooks": "^3.1.3",
        "@apollo/react-ssr": "^3.1.3",
        "@aws-amplify/auth": "^3.2.7",
        "@aws-amplify/core": "^3.2.7",
        "@next/bundle-analyzer": "^9.1.6",
        "@testing-library/jest-dom": "^4.2.4",
        "apollo-boost": "^0.4.7",
        "apollo-cache-inmemory": "^1.6.5",
        "apollo-server-micro": "^2.9.15",
        "aws-cognito-next": "^3.0.0",
        "aws-sdk": "^2.603.0",
        "axios": "^0.19.0",
        "babel-plugin-import-graphql": "^2.7.0",
        "dotenv": "^8.2.0",
        "framer-motion": "^1.10.3",
        "graphql-tag": "^2.10.1",
        "graphql-toolkit": "^0.7.5",
        "isomorphic-unfetch": "^3.0.0",
        "next": "^9.4.0",
        "query-string": "^6.12.1",
        "react-dom": "^16.13.1",
        "react-scripts": "3.4.1",
        "react-spinners": "^0.8.3",
        "react": "^16.13.1"
    },
    "devDependencies": {
        "@testing-library/react": "^10.0.4",
        "@types/jest": "^25.2.2",
        "@types/next": "^9.0.0",
        "@types/node": "^12.12.14",
        "@types/react": "^16.9.15",
        "@typescript-eslint/eslint-plugin": "^2.10.0",
        "@typescript-eslint/parser": "^2.10.0",
        "babel-plugin-module-resolver": "^4.0.0",
        "cross-env": "^6.0.3",
        "eslint-config-airbnb": "^18.0.1",
        "eslint-plugin-import": "^2.18.2",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-react-hooks": "^1.7.0",
        "eslint-plugin-react": "^7.17.0",
        "eslint": "^6.7.2",
        "husky": "^4.2.5",
        "jest-environment-jsdom-sixteen": "^1.0.3",
        "prettier": "^1.19.1",
        "pretty-quick": "^2.0.1",
        "serverless-next.js": "^1.7.0",
        "typescript": "^3.7.3"
    },
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged",
            "pre-push": "export CI=true && yarn run test"
        }
    }
}
