{
    "name": "backlogger",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
        "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
        "analyze": "cross-env ANALYZE=true next build",
        "build": "next build",
        "deploy:prod": "serverless deploy --stage prod",
        "dev": "next dev",
        "lint:pages": "eslint --fix './pages/**/*.{ts,tsx}'",
        "lint:src": "eslint --fix './src/**/*.{ts,tsx}'",
        "lint": "yarn run lint:src && yarn run lint:pages",
        "test:watch": "jest --watch --env=jest-environment-jsdom-sixteen",
        "test": "jest --env=jest-environment-jsdom-sixteen"
    },
    "dependencies": {
        "@apollo/react-hooks": "^3.1.3",
        "@apollo/react-ssr": "^3.1.3",
        "@aws-amplify/auth": "^3.2.12",
        "@aws-amplify/core": "^3.3.2",
        "@next/bundle-analyzer": "^9.4.4",
        "@testing-library/jest-dom": "^5.9.0",
        "apollo-boost": "^0.4.7",
        "apollo-cache-inmemory": "^1.6.5",
        "apollo-server-micro": "^2.14.2",
        "aws-cognito-next": "^3.0.0",
        "aws-sdk": "^2.691.0",
        "axios": "^0.19.0",
        "babel-plugin-import-graphql": "^2.7.0",
        "dotenv": "^8.2.0",
        "framer-motion": "^1.10.3",
        "graphql": "^15.0.0",
        "graphql-tag": "^2.10.1",
        "isomorphic-unfetch": "^3.0.0",
        "lodash": "^4.17.15",
        "merge-graphql-schemas": "^1.7.8",
        "next": "9.4.4",
        "query-string": "^6.13.0",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-select": "^3.1.0",
        "react-spinners": "^0.8.3",
        "uuid": "^8.1.0"
    },
    "devDependencies": {
        "@apollo/react-testing": "^3.1.4",
        "@testing-library/react": "^10.2.1",
        "@types/jest": "^25.2.2",
        "@types/next": "^9.0.0",
        "@types/node": "^14.0.11",
        "@types/react": "^16.9.35",
        "@typescript-eslint/eslint-plugin": "^3.1.0",
        "@typescript-eslint/parser": "^3.1.0",
        "cross-env": "^7.0.2",
        "eslint": "^7.2.0",
        "eslint-config-airbnb": "^18.0.1",
        "eslint-plugin-import": "^2.18.2",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-react": "^7.17.0",
        "eslint-plugin-react-hooks": "^4.0.4",
        "husky": "^4.2.5",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^26.0.1",
        "jest-environment-jsdom-sixteen": "^1.0.3",
        "prettier": "^2.0.5",
        "pretty-quick": "^2.0.1",
        "react-slick": "^0.26.1",
        "serverless-next.js": "^1.12.0",
        "slick-carousel": "^1.8.1",
        "typescript": "^3.9.5"
    },
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged",
            "pre-push": "export CI=true && yarn run test"
        }
    }
}
